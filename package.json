{
    "name": "jamalta-oidc-client",
    "version": "0.9.5",
    "description": "JA Malta OIDC Client for external/internal login-as implementation",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "umd:main": "dist/umd/index.js",
    "types": "dist/types/index.d.js",
    "scripts": {
        "build": "npm run build:cjs && npm run build:esm && npm run build:types",
        "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
        "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
        "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
        "dev-start": "node_modules/.bin/ts-node -r dotenv/config src/runner.ts"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "ja",
        "jamalta",
        "oauth",
        "openid",
        "framework",
        "authentication",
        "openid-relay"
    ],
    "author": {
        "name": "Jamie Stivala",
        "email": "jamie@jayemalta.org"
    },
    "homepage": "https://github.com/jamaltaorg/JA-Malta-OIDC-Client",
    "repository": {
        "type": "git",
        "url": "https://github.com/jamaltaorg/JA-Malta-OIDC-Client"
    },
    "bugs": {
        "url": "https://github.com/jamaltaorg/JA-Malta-OIDC-Client/issues"
    },
    "dependencies": {
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "openid-client": "^5.1.6",
        "typedoc": "^0.22.17"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.35",
        "cookie-parser": "^1.4.6",
        "ts-node": "^10.7.0",
        "typescript": "^4.7.2"
    }
}
